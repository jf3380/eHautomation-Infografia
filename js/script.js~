$(document).ready(function(){$("#sala").fadeTo("slow",.1),$("#signal").hide(),$("#encender-luz").show(),$("#encender-tv").show(),$("#isala").show(),$("#icocina").hide(),$("#iestar").hide(),$("#iporshe").hide(),$("#valor-temp").val(70),$("#temperatura").html("70 &deg;f"),$("#temp").change(function(){var i=$(this).val();$("#valor-temp").val(i),$("#temperatura").html(i+" &deg;f")}),$("#apa-luz").change(function(){var i=$(this).val();$("#sala").fadeTo("slow",i)}),$("#apagar-luz").click(function(){$("#sala").fadeTo("slow",.1),$("#apagar-luz").hide(),$("#encender-luz").show()}),$("#encender-luz").click(function(){$("#sala").fadeTo("slow",9),$("#apagar-luz").show(),$("#encender-luz").hide()}),$("#apagar-tv").click(function(){$("#signal").hide()}),$("#encender-tv").click(function(){$("#signal").show()}),$("#habitacion").click(function(){$("#tv").show(),$("#isala").show(),$("#icocina").hide(),$("#iestar").hide(),$("#iporshe").hide()}),$("#cocina").click(function(){$("#tv").hide(),$("#signal").hide(),$("#isala").hide(),$("#icocina").show(),$("#iestar").hide(),$("#iporshe").hide()}),$("#salaestar").click(function(){$("#tv").hide(),$("#signal").hide(),$("#isala").hide(),$("#icocina").hide(),$("#iporshe").hide(),$("#iestar").show()}),$("#porshe").click(function(){$("#tv").hide(),$("#signal").hide(),$("#isala").hide(),$("#icocina").hide(),$("#iporshe").show()}),$(document).bind("contextmenu",function(){return!1})});